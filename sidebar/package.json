    "rust",
    "webpack"
  ],
  "author": "Marco Stahl",
  "license": "AGPL",
  "bugs": {
    "url": "https://github.com/rustwasm/create-wasm-app/issues"
  },
  "homepage": "https://github.com/rustwasm/create-wasm-app#readme",
  "dependencies": {
    "@acrolinx/sidebar-interface": "^14.28.2",
    "binary-search-by": "^0.3.0",
    "franc-min": "^5.0.0",
    "htmlparser2": "^6.1.0",
    "nlprule-wasm": "file:../nlprule-wasm/pkg",
    "solid-js": "^1.0.3",
    "unionize": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.23",
