    "babel-preset-solid": "^0.26.5",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^5.2.6",
    "dotenv": "^10.0.0",
    "jest": "^27.0.5",
    "style-loader": "^3.0.0",
    "ts-jest": "^27.0.3",
    "typed-css-modules": "^0.7.0",
    "typescript": "^4.3.4",
    "webpack": "^5.39.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
